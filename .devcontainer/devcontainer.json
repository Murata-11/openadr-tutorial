{
    "name": "vtn servers Dev Container",
    "dockerComposeFile": "./docker-compose.yml",
    "service": "vtn",
    "workspaceFolder": "/workspaces",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.black-formatter"
            ],
            "settings": {
                "python.formatting.provider": "black",
                "editor.formatOnSave": true
            }
        }
    }
}
